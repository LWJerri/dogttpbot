    "@types/express": "^4.17.18",
    "@types/node": "^20.8.0",
    "prettier": "^3.0.3",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
